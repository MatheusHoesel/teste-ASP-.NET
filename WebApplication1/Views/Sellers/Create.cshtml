@model WebApplication1.Models.Seller
@{
    ViewData["Title"] = "Create";
}

<h2>@ViewData["Ttitle"]</h2>

<h4>Seller</h4>
<hr />
<div class="row">
    <div class="cold-md-4">
        <form asp-action="Create">
            <div class="from-group">
                <label asp-for="Name" class="control-label"></label>
                <input asp-for="Name" class="from-control" />
            </div>
            <div class="from-group">
                <label asp-for="Email" class="control-label"></label>
                <input asp-for="Email" class="from-control" />
            </div>
            <div class="from-group">
                <label asp-for="BirthDate" class="control-label"></label>
                <input asp-for="BirthDate" class="from-control" />
            </div>
            <div class="from-group">
                <label asp-for="BaseSalary" class="control-label"></label>
                <input asp-for="BaseSalary" class="from-control" />
            </div>
            <div class="from-group">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
